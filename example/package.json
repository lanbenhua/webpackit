{
  "name": "reaction-starter-kit",
  "version": "1.0.0",
  "description": "",
  "indexHash": "",
  "scripts": {
    "tunnel": "shopee-tunnel -p `pwd`/config/proxy.js",
    "start": "npm run tunnel && DEV_MODE=true webpack-dev-server",
    "eslint": "eslint --max-warnings 0 --ext .ts,.js ./",
    "swagger": "npx openapi-typescript http://localhost:9901/api/v2/api-docs --output ./src/types/swagger-type.ts",
    "test-build": "rm -rf dist && JENKINS_ENV=test NODE_ENV=production webpack",
    "uat-build": "rm -rf dist && JENKINS_ENV=uat NODE_ENV=production webpack",
    "staging-build": "rm -rf dist && JENKINS_ENV=staging NODE_ENV=production webpack",
    "build": "rm -rf dist && NODE_ENV=production NO_CONSOLE=true webpack"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@ant-design/icons": "4.4.0",
    "@shopee-data/data-tracking": "0.0.3-beta3",
    "@shopee-data/di-common-header-fe": "latest",
    "@shopee-data/micro-fe": "latest",
    "@shopee-data/reaction": "latest",
    "@shopee-data/tunnel": "latest",
    "@shopee-data/webpack-base-config": "latest",
    "@types/echarts": "^4.9.7",
    "@types/lodash": "^4.14.170",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "antd": "4.9.4",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "history": "^4.10.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run eslint"
    ],
    "*.{less,json,html}": [
      "prettier --write"
    ]
  }
}
